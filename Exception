x = input("x:")
y = input("y:")
try:
    x = int(x) 
    y = int(y) 
    res = x/y      #lines (4,5,6) - critical code               
except ZeroDivisionError :
    print('Divide on zero. Use numbers more zero')
except ValueError:
    print('Using of float number. Use only integer one!')
else:
    print("Виключення не виникли") # if in critical code all come good
finally:
    print("Блок finally завжди виконується")
